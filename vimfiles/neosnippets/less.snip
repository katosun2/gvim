snippet cssxz
	.transform-scale(@angel:45,@scale:1){
		@rad : ~`@{angel} * Math.PI / 180`;
		@m11 : ~`Math.cos(@{rad}) * @{scale}`;
		@m12 : ~`-1 * Math.sin(@{rad}) * @{scale}`;
		@m21 : ~`Math.sin(@{rad}) * @{scale}`;
		@m22 : @m11;
		@angelnew : ~`'@{angel}'+'deg'`;
		-webkit-transform:~"rotate("@angelnew~")" ~"scale("@scale~")";
		-moz-transform:~"rotate("@angelnew~")" ~"scale("@scale~")";
		-o-transform:~"rotate("@angelnew~")" ~"scale("@scale~")";
		filter : e(%("progid:DXImageTransform.Microsoft.Matrix(M11='%d',M12='%d',M21='%d',M22='%d',SizingMethod='auto expand')",@m11,@m12,@m21,@m22));
	}
snippet xz
	.transform-scale(${1:angel_45},${2:scale_1});
